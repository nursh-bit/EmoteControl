{
    "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
    "runtime": {
        "version": "Lua 5.1",
        "path": [
            "?.lua",
            "?/init.lua"
        ]
    },
    "diagnostics": {
        "enable": true,
        "severity": {
            "undefined-global": "Warning",
            "lowercase-global": "Error"
        },
        "globals": [
            "UIParent",
            "CreateFrame",
            "C_Timer",
            "C_Map",
            "C_NamePlate",
            "PixelUtil",
            "ViragDevTool_AddData",
            "C_AddOns",
            "LoadAddOn",
            "InCombatLockdown",
            "GetTime",
            "UnitName",
            "UnitHealth",
            "UnitHealthMax",
            "UnitPower",
            "UnitPowerMax",
            "UnitPowerType",
            "UnitLevel",
            "UnitRace",
            "UnitClass",
            "UnitFactionGroup",
            "GetRealmName",
            "GetGuildInfo",
            "GetZoneText",
            "GetSubZoneText",
            "GetSpecialization",
            "GetSpecializationInfo",
            "GetSpecializationRole",
            "GetAverageItemLevel",
            "UnitIsDeadOrGhost",
            "UnitExists",
            "IsInInstance",
            "GetInstanceInfo",
            "IsInRaid",
            "IsInGroup",
            "GetNumGroupMembers",
            "CombatLogGetCurrentEventInfo",
            "UnitGUID",
            "GetAchievementInfo",
            "C_Item",
            "C_Spell",
            "C_ChatInfo",
            "Minimap",
            "GameTooltip",
            "GetCursorPosition",
            "C_ChallengeMode",
            "C_MythicPlus",
            "AuraUtil",
            "UnitClassification",
            "UnitIsEnemy",
            "UnitIsFriend",
            "DEFAULT_CHAT_FRAME",
            "LE_PARTY_CATEGORY_INSTANCE",
            "LE_PARTY_CATEGORY_HOME",
            "wipe"
        ]
    },
    "workspace": {
        "checkThirdParty": true,
        "library": [
            "~/.vscode/extensions/ketho.wow-api-*"
        ]
    },
    "telemetry": {
        "enable": false
    }
}